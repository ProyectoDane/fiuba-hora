            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".SeccionesActivity" />
        </activity>
        <activity
            android:name=".IntroMediasHoras1Activity"
            android:parentActivityName=".SeccionesActivity"
